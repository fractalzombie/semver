{
  "name": "fractal/semver",
  "description": "The package helps to work with semantic versions.",
  "keywords": [
    "version",
    "semantic",
    "compare",
    "helpers"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Shtanko",
      "email": "fractalzombie@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "phpunit/php-code-coverage": "^6.1",
    "php-coveralls/php-coveralls": "^2.1",
    "codedungeon/phpunit-result-printer": "^0.23.4"
  },
  "autoload": {
    "psr-4": {
      "Fractal\\SemVer\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fractal\\SemVer\\Tests\\": "./tests"
    }
  }
}
